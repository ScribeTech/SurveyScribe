{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "mocha": true
  },
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-unused-vars": ["warn"]
  }
}
